{
   "$schema": "https://schemas.wp.org/trunk/theme.json",
   "version": 3,
   "settings": {
      "appearanceTools": false,
      "color": {
         "custom": false,
         "gradients": [],
         "defaultGradients": false,
         "customGradient": false,
         "duotone": [],
         "defaultDuotone": false,
         "customDuotone": false,
         "link": false,
         "text": false,
         "background": false,
         "defaultPalette": false,
         "palette": [
            {
					"color": "rgba(0,0,0,1)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "rgba(255,255,255,1)",
					"name": "Secondary",
					"slug": "secondary"
				},
            { 
               "slug": "red",
               "name": "Red",
               "color": "#ff0000"
            }
         ]
      },
      "border": {
         "radius": false,
         "width": false
      },
      "typography": {
         "fontSizes": [],
         "customFontSize": false,
         "lineHeight": false,
         "letterSpacing": false,
         "textDecoration": false,
         "textTransform": false,
         "fontWeight": false
      },
      "blocks": {
         "core/heading": {
            "typography": {
               "textDecoration": false
            }
         },
         "core/button": {
            "color": {
               "background": false,
               "text": false,
               "defaultPalette": false
            },
            "border": {
               "radius": false,
               "width": false
            },
            "typography": {
               "fontSizes": [],
               "customFontSize": false,
               "lineHeight": false,
               "letterSpacing": false
            },
            "shadow": {
               "defaultPresets": false
            }
         }
      }
   },
   "styles": {
      "color": {
         "background": "var(--wp--preset--color--primary)",
         "text": "var(--wp--preset--color--secondary)"
      }
   },
   "templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "release",
			"title": "Release"
		}
	],
	"customTemplates": [
		{
			"name": "single-release",
			"postTypes": ["release"],
			"title": "Release Page"
		}
	]
}