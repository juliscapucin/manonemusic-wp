{
   "$schema": "https://schemas.wp.org/trunk/theme.json",
   "version": 3,
   "settings": {
      "appearanceTools": true,
      "spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "1",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "2",
					"size": "min(1.5rem, 2vw)",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "min(2.5rem, 3vw)",
					"slug": "30"
				},
				{
					"name": "4",
					"size": "min(4rem, 5vw)",
					"slug": "40"
				},
				{
					"name": "5",
					"size": "min(6.5rem, 8vw)",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "min(10.5rem, 13vw)",
					"slug": "60"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
      "color": {
         "custom": false,
         "gradients": [],
         "defaultGradients": false,
         "customGradient": false,
         "duotone": [],
         "defaultDuotone": false,
         "customDuotone": false,
         "link": false,
         "text": false,
         "background": false,
         "defaultPalette": false,
         "palette": [
            {
					"color": "rgba(0,0,0,1)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "rgba(255,255,255,1)",
					"name": "Secondary",
					"slug": "secondary"
				},
            { 
               "slug": "red",
               "name": "Red",
               "color": "#ff0000"
            }
         ]
      },
      "border": {
         "radius": false,
         "width": false
      },
      "typography": {
         "fontSizes": [],
         "customFontSize": false,
         "lineHeight": false,
         "letterSpacing": false,
         "textDecoration": false,
         "textTransform": false,
         "fontWeight": false
      },
      "blocks": {
         "core/heading": {
            "typography": {
               "textDecoration": false
            }
         },
         "core/button": {
            "color": {
               "background": false,
               "text": false,
               "defaultPalette": false
            },
            "border": {
               "radius": false,
               "width": false
            },
            "typography": {
               "fontSizes": [],
               "customFontSize": false,
               "lineHeight": false,
               "letterSpacing": false
            },
            "shadow": {
               "defaultPresets": false
            }
         }
      }
   },
   "styles": {
      "color": {
         "background": "var(--wp--preset--color--primary)",
         "text": "var(--wp--preset--color--secondary)"
      }
   },
   "templateParts": [
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
      {
			"area": "header",
			"name": "header",
			"title": "Header"
		}
	],
	"customTemplates": [
      {
         "name": "template-test",
         "postTypes": ["release"],
         "title": "Template Test"
      }
	]
}