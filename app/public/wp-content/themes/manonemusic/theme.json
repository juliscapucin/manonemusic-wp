{
   "$schema": "https://schemas.wp.org/trunk/theme.json",
   "version": 3,
   "settings": {
      "appearanceTools": false,
      "color": {
         "custom": false,
         "customGradient": false,
         "customDuotone": false,
         "duotone": [],
         "gradients": [],
         "link": false,
         "text": false,
         "background": false,
         "defaultPalette": false,
         "defaultGradients": false,
         "palette": [
            {
					"color": "#f9f9f9",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#ffffff",
					"name": "Contrast",
					"slug": "contrast"
				}
         ]
      },
      "border": {
         "radius": false,
         "width": false
      },
      "typography": {
         "fontSizes": [],
         "customFontSize": false,
         "lineHeight": false,
         "letterSpacing": false,
         "textDecoration": false,
         "textTransform": false,
         "fontWeight": false
      },
      "blocks": {
         "core/heading": {
            "typography": {
               "textDecoration": false
            }
         },
         "core/button": {
            "color": {
               "background": false,
               "text": false,
               "defaultPalette": false
            },
            "border": {
               "radius": false,
               "width": false
            },
            "typography": {
               "fontSizes": [],
               "customFontSize": false,
               "lineHeight": false,
               "letterSpacing": false
            },
            "shadow": {
               "defaultPresets": false
            }
         }
      }
   },
   "styles": {
      "color": {
         "background": "var(--wp--preset--color--base)",
         "text": "var(--wp--preset--color--contrast)"
      }
   }
}