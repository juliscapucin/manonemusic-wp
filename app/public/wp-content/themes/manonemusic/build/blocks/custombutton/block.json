{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "manonemusic/custombutton",
  "title": "Custom Button",
  "category": "layout",
  "keywords": [
    "button",
    "custom"
  ],
  "attributes": {
    "text": {
      "type": "string"
    },
    "linkObject": {
      "type": "object",
      "default": {
        "url": ""
      }
    },
    "className": {
      "type": "string",
      "default": "bg-red-400 p-2 w-fit"
    },
    "isExternal": {
      "type": "boolean",
      "default": false
    }
  },
  "icon": "button",
  "description": "Custom Button",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}