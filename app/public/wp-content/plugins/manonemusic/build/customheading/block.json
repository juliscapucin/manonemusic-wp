{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "manonemusic/custom-heading",
  "version": "0.1.0",
  "title": "Custom Heading",
  "category": "text",
  "icon": "heading",
  "description": "Custom heading",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "text": {
      "type": "string"
    },
    "size": {
      "type": "string",
      "default": "text-9xl"
    },
    "tag": {
      "type": "string",
      "default": "h1"
    }
  },
  "textdomain": "custom-heading",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}