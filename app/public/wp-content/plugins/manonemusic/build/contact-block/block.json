{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "manonemusic/contact-block",
  "version": "0.1.0",
  "title": "Contact Block",
  "category": "design",
  "icon": "contact",
  "description": "Block for Contact Page",
  "example": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "Contact"
    },
    "paragraph": {
      "type": "string",
      "default": "Contact us for more information"
    },
    "align": {
      "type": "string",
      "default": "full"
    }
  },
  "supports": {
    "align": [
      "full"
    ]
  },
  "textdomain": "contact-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}