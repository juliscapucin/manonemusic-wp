{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "home-block/home-block",
  "version": "0.1.0",
  "title": "Home Block",
  "category": "design",
  "icon": "home",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "slogan": {
      "type": "string",
      "default": "Music & Sound Design"
    },
