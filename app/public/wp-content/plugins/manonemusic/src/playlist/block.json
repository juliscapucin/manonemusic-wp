{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "manonemusic/playlist",
	"version": "0.1.0",
	"title": "Playlist",
	"category": "widgets",
	"icon": "smiley",
	"description": "Playlist block for Manone Music",
	"example": {},
	"supports": {
		"html": false,
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"classes": {
			"type": "string",
			"default": ""
		},
		"count": {
			"type": "number",
			"default": 1
		},
		"tracklist": {
			"type": "array",
			"default": [{"title": "Track 1", "url": ""}]
		}
	},
	"textdomain": "playlist",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}